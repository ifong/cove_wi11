<ul class="browse_cont">
   <li class="grid_14 title">Title of session goes here</li>
   <li class="grid_2 number alpha omega">FAR <%= interval.id.to_s() %></li>
   <li class="grid_3">Video here</li> 
   <li class="grid_2 box"> 
		<% if !interval.start_time.blank?%>
       <ul>
           <li class="calendar-top"><%= interval.start_time.strftime("%b") %> <%= interval.start_time.strftime("%Y") %></li>
           <li><%= interval.start_time.strftime("%A") %></li>
           <li class="calendar-day"><%= interval.start_time.strftime("%d") %></li>
           <li><%= interval.start_time.strftime("%I") %>:<%= interval.start_time.strftime("%M") %> <%= interval.start_time.strftime("%p") %></li>
           <li></li>
       </ul>
		<% end %>
   </li>
   <li class="grid_2 box camera" 
     style="background: 
     url('/images/t-wayne.png')
     no-repeat top center;"><%= interval.camera_angle %></li> 
   <li class="grid_2 box omega"> 
        <ul>
	   <li class="label">Phrase name</li><li> <%= interval.phrase_name  %></li>
	   <li class="label">Phrase type</li><li> <%= interval.phrase_type %></li>
        </ul>
   </li>
   <li class="grid_2 box omega"><ul><li class="label">Session type</li><li><span class="session_type define_me"><%= interval.session_type %></span></li></ul></li> 
   <li class="grid_5 box tags omega"><ul><li class="label">Tags</li><li>tags, tags, tags, tags, tags, tags, tags, tags, tags, tags, tags, tags, tags, tags, tags</li></ul>
</li> 
   <br class="clear">
</ul> 
